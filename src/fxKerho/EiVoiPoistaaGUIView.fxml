<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxKerho.EiVoiPoistaaGUIController">
   <center>
      <VBox alignment="CENTER" prefHeight="150.0" prefWidth="220.0" styleClass="vaaleaVihrea" BorderPane.alignment="CENTER">
         <children>
            <Label text="Tasoituskierrosta ei voi poistaa!">
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </Label>
            <Button mnemonicParsing="false" onAction="#handleOK" text="OK">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </Button>
         </children>
      </VBox>
   </center>
</BorderPane>
